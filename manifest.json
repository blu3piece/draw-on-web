{
    "manifest_version": 3,
    "name": "CRXJS React Vite Example",
    "version": "1.0.0",
    "icons": {
        "16": "src/assets/icon.png",
        "32": "src/assets/icon.png",
        "48": "src/assets/icon.png",
        "128": "src/assets/icon.png"
      },
    "action": { "default_popup": "index.html" },
    "content_scripts": [
        {
          "js": ["src/content.tsx"],
          "matches": ["https://www.google.com/*"]
        }
      ]
  }